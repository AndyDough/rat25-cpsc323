" This is a comprehensive test case for the Rat25F parser "

function comprehensive_test(is_active boolean, scale real) 
    " This function tests various features of the language "
    integer i, j;
    real r;
    boolean done;
{
    i = 0;
    j = 10;
    done = false;
    r = 1.0;

    get(i); " Read a value for i "

    while (i < j) {
        if (i == 5) {
            put(i);
        } else {
            if (i != 2) {
                r = (r * scale) + (i / 2.0);
                put(r);
            } fi
        } fi
        
        i = i + 1;
        
        if (i > 8) {
            done = true;
        } fi
    }
}

